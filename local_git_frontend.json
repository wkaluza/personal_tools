      "image": "${REVERSE_PROXY_IMAGE_REFERENCE:?}",
      "labels": {
        "wk.connect.cluster-cnr8lm0i": "true"
      },
      "logging": {
        "driver": "json-file",
        "options": {
          "max-file": 5,
          "max-size": "200k"
        }
      },
      "networks": [
        "external_add45a30",
        "internal_git_frontend_3zmkeoim"
